#include <stdio.h>

int main() {
    int n, primeiro = 0, segundo = 1, proximo;

    printf("Digite a quantidade de termos da serie de Fibonacci: ");
    scanf("%d", &n);

    printf("Serie de Fibonacci:\n");

    for (int i = 0; i < n; i++) {
        if (i <= 1) {
            proximo = i;
        } else {
            proximo = primeiro + segundo;
            primeiro = segundo;
            segundo = proximo;
        }
        printf("%d ", proximo);
    }

    printf("\n");
    return 0;
}
