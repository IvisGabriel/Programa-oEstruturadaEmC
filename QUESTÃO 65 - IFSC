#include <stdio.h>

int main() {
    float nota1, nota2, media;

    // Leitura das notas
    printf("Digite a primeira nota: ");
    scanf("%f", &nota1);

    printf("Digite a segunda nota: ");
    scanf("%f", &nota2);

    // Cálculo da média
    media = (nota1 + nota2) / 2;

    // Verificação de aprovação
    printf("Media: %.2f\n", media);

    if (media > 5.0) {
        printf("Aluno APROVADO!\n");
    } else {
        printf("Aluno REPROVADO!\n");
    }

    return 0;
}
