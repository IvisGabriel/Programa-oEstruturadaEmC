#include <stdio.h>

int main() {
    // Declaração da variável para armazenar a temperatura em Celsius
    float C, F;

    // Solicitar ao usuário que insira a temperatura em Celsius
    printf("Digite a temperatura em graus Celsius: ");
    scanf("%f", &C);

    // Fórmula de conversão de Celsius para Fahrenheit
    F = (9 * C + 160) / 5;

    // Exibir o resultado da conversão
    printf("A temperatura em Fahrenheit é: %.2f\n", F);

    return 0;
}
