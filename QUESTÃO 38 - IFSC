#include <stdio.h>

int main() {
    int A, B, C;
    
    // Leitura dos valores
    printf("Digite o valor de A: ");
    scanf("%d", &A);
    
    printf("Digite o valor de B: ");
    scanf("%d", &B);
    
    printf("Digite o valor de C: ");
    scanf("%d", &C);
    
    // Comparação da soma de A e B com C
    if (A + B < C) {
        printf("A + B é menor que C.\n");
    } else if (A + B > C) {
        printf("A + B é maior que C.\n");
    } else {
        printf("A + B é igual a C.\n");
    }

    return 0;
}
