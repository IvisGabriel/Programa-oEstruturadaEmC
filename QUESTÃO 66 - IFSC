#include <stdio.h>

int main() {
    int num1, num2;

    // Leitura dos números
    printf("Digite o primeiro numero: ");
    scanf("%d", &num1);

    printf("Digite o segundo numero: ");
    scanf("%d", &num2);

    // Verificação e exibição do maior e menor
    if (num1 > num2) {
        printf("Maior: %d\n", num1);
        printf("Menor: %d\n", num2);
    } else if (num2 > num1) {
        printf("Maior: %d\n", num2);
        printf("Menor: %d\n", num1);
    } else {
        printf("Os dois numeros sao iguais: %d\n", num1);
    }

    return 0;
}
