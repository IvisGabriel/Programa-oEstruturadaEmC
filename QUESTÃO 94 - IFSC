#include <stdio.h>
int main() {
    int A, B, modulo;
    // a) 10 vezes
    for (int i = 0; i < 10; i++) {
        scanf("%d %d", &A, &B);
        modulo = A % B;
        printf("Módulo: %d\n", modulo);
    }

    // b) Nenhuma vez
    for (int i = 0; i < 0; i++) {
        // não executa
    }

    // c) 100 vezes com 2 repetições
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 10; j++) {
            scanf("%d %d", &A, &B);
            modulo = A % B;
            printf("Módulo: %d\n", modulo);
        }
    }

    // d) N vezes
    int N;
    printf("Digite o número de repetições: ");
    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        scanf("%d %d", &A, &B);
        modulo = A % B;
        printf("Módulo: %d\n", modulo);
    }
    return 0;
}
