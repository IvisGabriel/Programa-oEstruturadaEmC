a) 6 <= 7
#include <stdio.h>

int main() {
    // Expressão a)
    if (6 <= 7) {
        printf("a) 6 <= 7: VERDADEIRO\n");
    } else {
        printf("a) 6 <= 7: FALSO\n");
    }
    return 0;
}

a) 6 <= 7: VERDADEIRO


b) (64) ^ (1/2) > (5)*2

#include <stdio.h>
#include <math.h>

int main() {
    // Expressão b)
    if (pow(64, 1.0/2) > (5) * 2) {
        printf("b) (64) ^ (1/2) > (5)*2: VERDADEIRO\n");
    } else {
        printf("b) (64) ^ (1/2) > (5)*2: FALSO\n");
    }
    return 0;
}

b) (64) ^ (1/2) > (5)*2: FALSO


c) 'Pseudocódigo' < 'PSEUDOCÓDIGO'

#include <stdio.h>

int main() {
    // Expressão c)
    if ("Pseudocódigo" < "PSEUDOCÓDIGO") {
        printf("c) 'Pseudocódigo' < 'PSEUDOCÓDIGO': VERDADEIRO\n");
    } else {
        printf("c) 'Pseudocódigo' < 'PSEUDOCÓDIGO': FALSO\n");
    }
    return 0;
}

c) 'Pseudocódigo' < 'PSEUDOCÓDIGO': VERDADEIRO


e) 3 + 5 <= 2 * 4

#include <stdio.h>

int main() {
    // Expressão e)
    if ((3 + 5) <= (2 * 4)) {
        printf("e) 3 + 5 <= 2 * 4: VERDADEIRO\n");
    } else {
        printf("e) 3 + 5 <= 2 * 4: FALSO\n");
    }
    return 0;
}

e) 3 + 5 <= 2 * 4: VERDADEIRO

