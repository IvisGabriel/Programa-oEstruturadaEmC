#include <stdio.h>

int main() {
    int a, i, contador_negativos = 0;

    for (i = 1; i <= 5; i++) {
        printf("Digite o %dÂº valor: ", i);
        scanf("%d", &a);

        if (a < 0) {
            contador_negativos++;
        }
    }

    printf("Quantidade de valores negativos: %d\n", contador_negativos);
