#include <stdio.h>

int main() {
    float a, b, c;

    // Leitura dos valores
    printf("Digite o valor de a: ");
    scanf("%f", &a);

    printf("Digite o valor de b: ");
    scanf("%f", &b);

    printf("Digite o valor de c: ");
    scanf("%f", &c);

    // Exibe os valores digitados
    printf("Valores digitados: a = %.2f, b = %.2f, c = %.2f\n", a, b, c);

    // Verifica e exibe o maior valor
    if (a >= b && a >= c) {
        printf("%.2f eh o maior\n", a);
    } else if (b >= a && b >= c) {
        printf("%.2f eh o maior\n", b);
    } else {
        printf("%.2f eh o maior\n", c);
    }

    return 0;
}
